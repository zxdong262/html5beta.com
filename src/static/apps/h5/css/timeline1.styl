.paper
	display none

.tab-contents
	padding 0 25px 25px
	h2
		font-size 30px
		margin-top 45px


.time-sections
	padding 25px 0
	position relative
	&:before
		content ""
		position absolute
		left 0
		width 3px
		top 15px
		bottom 15px
		background #999
		background rgba(0,0,0,0.1)
		
.time-section
	margin 0 0 10px
	position relative
	&:before
		content ""
		position absolute
		top 4px
		left -6px
		width 15px
		height 15px
		border 3px solid #fff
		border-radius 40px
		box-shadow 0 1px 1px rgba(0,0,0,0.2), inset 0 1px 1px rgba(0,0,0,0.1)
		background #ccc
	&:hover
		&:before
			background orange
	.times
		padding 6px 12px
		position relative
		background #f9f9f9
		border-radius 4px
		margin-left 24px
		font-size 14px
		font-weight bold
		color #999
		&:before
			border-color transparent #f9f9f9
			border-style solid
			border-width 5px 8px 5px 0
			content ""
			display block
			left -8px
			position absolute
			top 9px
			width 0
		.arr
			padding 0 5px
		.expand
			position absolute
			right 10px
			top 6px

	.projs,
	.desc
		margin-left 30px
		padding 10px
		word-break break-all

	&.compact .projs,
	&.compact .desc
		display none