{
  "name": "html5beta.com",
  "version": "0.0.1",
  "private": true,
  "description": "html5beta.com",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "vercel dev",
    "b": "npm run rm && npm run b2 && npm run b1 && npm run cp && npm run sitemap",
    "b1": "node bin/build-pug-legacy.js",
    "b2": "node bin/build.js",
    "sitemap": "node bin/sitemap.js",
    "build": "echo \"ok\"",
    "fix": "standard --fix",
    "lint": "standard",
    "pub": "vercel --prod",
    "cp": "cp -r src/static/* public/",
    "rm": "rm -rf public/ && mkdir public",
    "c": "webpack serve --config bin/webpack/webpack.config.js",
    "bb": "NODE_ENV=production node_modules/.bin/webpack --progress --config bin/webpack/webpack.config.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/eslint-parser": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@tsconfig/recommended": "^1.0.2",
    "@types/node": "^20.3.1",
    "@vercel/node": "^2.15.8",
    "antd-mobile": "^5.32.0",
    "antd-mobile-icons": "^0.3.0",
    "axios": "^1.4.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.44.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "import-jsx": "^5.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "morgan": "^1.10.0",
    "nanoid": "^4.0.2",
    "pug": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "shelljs": "^0.8.5",
    "sitemaps": "^2.0.4",
    "standard": "^17.1.0",
    "stylus-loader": "^7.1.3",
    "typescript": "^5.1.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zxdong262/html5beta.com.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zxdong262/html5beta.com/issues"
  },
  "homepage": "https://github.com/zxdong262/html5beta.com#readme",
  "standard": {
    "ignore": [
      "/temp",
      "/public"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "sourceType": "module",
      "plugins": [
        "importAssertions"
      ]
    }
  },
  "dependencies": {
    "@roudanio/html2pug": "^1.0.2"
  }
}
